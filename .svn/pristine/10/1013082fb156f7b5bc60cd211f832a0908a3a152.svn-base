#!/bin/tcsh

echo "\n======"
echo "User:       " `whoami`
echo "Groups:     " `groups`
echo "======\n"

set geant_dir = `echo $PREXDIR`

source $geant_dir/env_jlabcue

#$geant_dir/bin/Linux-g++/moller_sim $geant_dir/prex_prerun.mac $geant_dir/run_prex.mac CHIPS
$geant_dir/bin/Linux-g++/moller_sim $geant_dir/prex_prerun.mac $geant_dir/run_prex.mac QGSP_BERT_HP
#$geant_dir/bin/Linux-g++/moller_sim $geant_dir/prex_prerun.mac $geant_dir/run_prex.mac 

set my_rndm_file = currentEvent.rndm
set my_results_file = results_geant.txt

set my_root_file = `find . -name \*root`
echo "Trying to find rootfile... '$my_root_file' "
if ("$my_root_file" != "") then
    echo "Moving '$my_root_file' "
    mv $my_root_file ~/scratch/ROOTfiles/.
endif

rm $my_rndm_file $my_results_file

exit
