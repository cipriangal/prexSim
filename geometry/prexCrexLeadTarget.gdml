    <tube aunit="deg" startphi="0" deltaphi="360" lunit="cm" name="new_sc_crex_1" rmin="0.0" rmax="outer_r_new_sc - thick_new_sc" z="height_new_sc" />
    
    <tube aunit="deg" startphi="0" deltaphi="360" lunit="cm" name="new_sc_crex_water_tgt_0" rmin="0.0" rmax="outer_r_new_sc_water_tgt" z="height_new_sc_water_tgt" />
    <tube aunit="deg" startphi="0" deltaphi="360" lunit="cm" name="new_sc_crex_water_tgt_1" rmin="0.0" rmax="outer_r_new_sc_water_tgt - thick_new_sc_water_tgt" z="height_new_sc_water_tgt" />
    

